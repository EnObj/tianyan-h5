{
  "envId": "xxx",
  "$schema": "https://framework-1258016615.tcloudbaseapp.com/schema/latest.json",
  "functionRoot": "./functions",
  "functions": [
    {
      "name": "loadWebDocument",
      "timeout": 30,
      "envVariables": {},
      "runtime": "Nodejs10.15",
      "memorySize": 128,
      "handler": "index.main"
    },
    {
      "name": "notifyTyMessage",
      "timeout": 30,
      "envVariables": {
        "MAILER_AUTH_USER": "xxx@qq.com",
        "MAILER_AUTH_PASS": "xxxxxx",
        "WEBSITE_HOST": "https://xxx.com"
      },
      "runtime": "Nodejs10.15",
      "memorySize": 128,
      "handler": "index.main",
      "triggers": [
        {
          "name": "tyNotifyTyMessageTrigger",
          "type": "timer",
          "config": "0 0/3 * * * * *"
        }
      ]
    },
    {
      "name": "readAllTyUserMessage",
      "timeout": 3,
      "envVariables": {},
      "runtime": "Nodejs10.15",
      "memorySize": 128,
      "handler": "index.main"
    },
    {
      "name": "resolveTyChannel",
      "timeout": 30,
      "envVariables": {},
      "runtime": "Nodejs10.15",
      "memorySize": 128,
      "handler": "index.main"
    },
    {
      "name": "tyChannelListener",
      "timeout": 30,
      "envVariables": {},
      "runtime": "Nodejs10.15",
      "memorySize": 128,
      "handler": "index.main",
      "triggers": [
        {
          "name": "tyChannelListenerTrigger",
          "type": "timer",
          "config": "0/30 * * * * * *"
        }
      ]
    }
  ],
  "framework": {
    "name": "node-starter",
    "plugins": {
      "function": {
        "use": "@cloudbase/framework-plugin-function",
        "inputs": {}
      }
    }
  }
}